{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ClinicalTrialMetadata",
    "type": "object",
    "properties": {
        "title": {
            "type": "string"
        },
        "startDate": {
            "type": "string",
            "format": "date"
        },
        "endDate": {
            "type": "string",
            "format": "date"
        },
        "participants": {
            "type": "integer",
            "minimum": 1
        },
        "status": {
            "type": "string",
            "enum": [
                "Not Started",
                "Ongoing",
                "Completed"
            ]
        }
    },
    "required": [
        "title",
        "startDate",
        "status"
    ],
    "additionalProperties": false
}